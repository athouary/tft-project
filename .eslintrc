{
  "extends": [
    "standard",
    "standard-jsx",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:react/recommended"
  ],
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "react/prop-types": ["off"]
  },
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
      "sourceType": "module"
    }
  }
}
